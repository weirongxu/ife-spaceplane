{
  "name": "ife2016-spaceplane",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server -d --inline --hot --output-public-path='/assets/'",
    "lint": "eslint src",
    "deploy": "npm run build && gh-pages -d build",
    "build:clean": "rm -rf build",
    "build:webpack": "webpack -p --output-public-path='./assets/'",
    "build:html": "cp index.html build/index.html",
    "build": "npm run build:clean && npm run build:webpack && npm run build:html"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.4",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.4.0",
    "eventemitter2": "^1.0.0",
    "file-loader": "^0.8.5",
    "gh-pages": "^0.11.0",
    "jquery": "^2.2.2",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.2",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
